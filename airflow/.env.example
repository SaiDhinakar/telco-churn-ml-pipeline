# Airflow User ID
AIRFLOW_UID=1000

# Airflow Configuration
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW__CORE__FERNET_KEY=

# Database Configuration
POSTGRES_USER=airflow
POSTGRESS_PASSWORD=airflow
POSTGRES_DB=airflow

# External Services Configuration
# MLflow Tracking Server URL (can be IP, domain, or localhost)
MLFLOW_TRACKING_URI=http://localhost:5000

# Airflow Public URL (for API access from external services)
AIRFLOW_PUBLIC_URL=http://localhost:8080

# API Server Configuration (if using separate API deployment)
# The URL where the prediction API is accessible
API_SERVER_URL=http://localhost:8000